{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "webapp": {
      "root": "",
      "sourceRoot": "webapp",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "target/webapp",
            "index": "webapp/index.html",
            "main": "webapp/main.ts",
            "polyfills": "webapp/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "webapp/favicon.ico",
              "webapp/assets"
            ],
            "styles": [
              "webapp/assets/vendor/bootstrap/scss/bootstrap.scss",
              "node_modules/datatables.net-dt/css/jquery.dataTables.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.css",
              "webapp/assets/vendor/icomoon/style.css",
              "webapp/assets/scss/app-components.scss",
              "webapp/assets/vendor/template/mobilefirst/scss/app-templates.scss",
              "webapp/assets/scss/application.scss",
              "node_modules/@videogular/ngx-videogular/fonts/videogular.css",
              "node_modules/@ng-select/ng-select/themes/default.theme.css",
              "node_modules/aos/dist/aos.css"
            ],
            "scripts": [
              {
                "input": "node_modules/document-register-element/build/document-register-element.js",
                "inject": true
              },
              {
                "input": "node_modules/jquery/dist/jquery.js",
                "inject": true
              },
              {
                "input": "node_modules/datatables.net/js/jquery.dataTables.js",
                "inject": true
              },
              {
                "input": "node_modules/popper.js/dist/umd/popper.js",
                "inject": true
              },
              {
                "input": "webapp/assets/vendor/bootstrap/js/bootstrap.min.js",
                "inject": true
              },
              {
                "input": "node_modules/aos/dist/aos.js",
                "inject": true
              },
              {
                "input": "webapp/assets/vendor/datatable/js/jquery.spring-friendly.js",
                "inject": true
              }
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "webapp/environments/environment.ts",
                  "with": "webapp/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "qa": {
              "fileReplacements": [
                {
                  "replace": "webapp/environments/environment.ts",
                  "with": "webapp/environments/environment.qa.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "uat": {
              "fileReplacements": [
                {
                  "replace": "webapp/environments/environment.ts",
                  "with": "webapp/environments/environment.uat.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "webapp:build",
            "port": 4600
          },
          "configurations": {
            "production": {
              "browserTarget": "webapp:build:production"
            },
            "qa": {
              "browserTarget": "webapp:build:qa"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "webapp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "webapp/test.ts",
            "polyfills": "webapp/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "styles": [
              "webapp/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "webapp/favicon.ico",
              "webapp/assets"
            ]
          }
        }
      }
    },
    "webapp-e2e": {
      "root": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "protractor.conf.js",
            "devServerTarget": "webapp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "webapp:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "webapp",
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "cli": {
    "analytics": false
  }
}